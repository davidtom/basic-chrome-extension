{
    // Required
    
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "manifest_version": 2,
    "name": "basic-chrome-extension",
    "version": "0.0.1",
    // Recommended
    // "default_locale": "en",
    "description": "Boilerplate for an extension built with react and redux",
    "browser_action": {
        "default_title": "Basic Example",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "css": [],
            "js": ["content.js"]
        }
    ],
